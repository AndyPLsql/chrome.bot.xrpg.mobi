{
  "name": "Orc killer",
  "version": "0.2.9",
  "manifest_version": 2,

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
          "storage"
  ],

  "content_scripts": [
  {
    "matches": ["http://xrpg.mobi/*"],
    "js": ["content.js"],
    "run_at": "document_end"
  }
  ],
  "web_accessible_resources": ["*.js"]
}